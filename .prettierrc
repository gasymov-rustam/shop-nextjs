{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "printWidth": 120,
  "bracketSpacing": true,
  "trailingComma": "all",
  "importOrder": [
    "^components/(.*)$",
    "^../(.*)$",
    "^\\./(.*)$",
    "^(.)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "endOfLine": "auto"
}
